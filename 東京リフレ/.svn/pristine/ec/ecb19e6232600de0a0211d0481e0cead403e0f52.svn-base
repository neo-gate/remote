	<div id="pate_top_therapist_name">
	{{$params.therapist_name}}さん
	</div>
	{{if $params.area == "tokyo_reraku"}}
		{{include file="top_menu_reraku.tpl"}}
	{{else}}
		{{include file="top_menu.tpl"}}
	{{/if}}
	{{if $params.error != ""}}
		<div style="text-align:center;padding-top:5px;">{{$params.error}}</div>
	{{/if}}
	<form action="" method="post" id="edit_frm">
		<div>
			<div class="title_bar4_2">
				<div class="left_1"><img src="{{$params.url_root_site}}img/shift/title/01_heading.gif" width="140" /></div>
				<div class="left_2">売上&nbsp;{{$params.price_all}}&nbsp;円</div>
				<br class="clear" />
			</div>
			<div id="work_end_input">
				<div class="content_1">売上詳細</div>
				<div class="content_2">
					<table style="width:266px;margin: 0 auto;text-align: center;">
						<tr>
							<th style="background-color:gray;">項目A</th>
							<th style="background-color:gray;">確認</th>
							<th style="background-color:gray;">項目B</th>
							<th style="background-color:gray;">金額</th>
							<th style="background-color:gray;">顧客名</th>
						</tr>
						{{section name=cnt loop=$params.reservation_data}}
							<tr>
								<td><label>売上</label></td>
								{{if $params.reservation_data[cnt].card_flg == "1"}}
									<td><input name="chkprice[{{$smarty.section.cnt.index}}]" type="hidden" value="0"></td>
									<td>カード</td>
								{{else}}
									<td>
										<input class="check" data-price="price{{$smarty.section.cnt.index}}" id="" name="chkprice[{{$smarty.section.cnt.index}}]" type="checkbox" value="1">
									</td>
									<td>現金</td>
								{{/if}}
								<td>
									<input class="price" id="price{{$smarty.section.cnt.index}}" name="price{{$smarty.section.cnt.index}}" type="text" value="{{$params.reservation_data[cnt].price}}" readonly style="width:50px; color:skyblue;">
								</td>
								<td>{{$params.reservation_data[cnt].customer_name|mb_substr:0:3:'utf-8'|escape}} 様</td>
							</tr>
						{{/section}}
						{{section name=cnt loop=$params.movement_cost_data}}
							<tr>
								<td><label>移動実費</label</td>
								<td>
									<input class="check" data-price="spendprice{{$smarty.section.cnt.index}}" id="" name="chkspdprice[{{$smarty.section.cnt.index}}]" type="checkbox" value="-1">
								</td>
								<td>
									{{if $params.movement_cost_data[cnt].movement_method == "1"}}タクシー
									{{else if $params.movement_cost_data[cnt].movement_method == "2"}}電車
									{{/if}}
								</td>
								<td><input class="price" id="spendprice{{$smarty.section.cnt.index}}" name="spendprice{{$smarty.section.cnt.index}}" type="text" value="-{{$params.movement_cost_data[cnt].cost_value}}" readonly style="width:50px; color:red;"></td>
								<td></td>
							</tr>
						{{/section}}
					</table>
				</div>
				<div>
					<p style="width:170px; margin:0 auto;">提出合計金額：<input id="priceTotal" name="priceTotal" type="text" style="width:70px; color:skyblue;"></p>
				</div>
				<br>
				<p><input name="check_confirm" type="checkbox" value="1">　移動実費の入力、領収書の提出漏れはありません。</td></p>
				<div class="content_5">売上、施術バッグ</div>
				<div class="content_6">
					{{if $params.money_bag_type == "1"}}
						<input type="radio" name="money_bag_type" value="1" checked="checked">
					{{else}}
						<input type="radio" name="money_bag_type" value="1">
					{{/if}}
						事務所に戻しました
				</div>
				<div class="content_7">
					{{if $params.money_bag_type == "2"}}
						<input type="radio" name="money_bag_type" value="2" checked="checked">
					{{else}}
						<input type="radio" name="money_bag_type" value="2">
					{{/if}}
					ドライバー{{$params.driver_select_frm}}さんに預けました
				</div>
				<div class="content_8">連絡事項</div>
				<div class="content_9"><textarea name="kanso" class="work_end_kanso_textarea">{{$params.kanso}}</textarea></div>
				<div class="content_10">
					<div class="left_1">
						<img src="{{$params.url_root_site}}img/shift/btn/05_button.gif" width="135" class="image_btn" onclick="submit_btn_click_action('send_back');" />
					</div>
					<div class="left_2">
						<img src="{{$params.url_root_site}}img/shift/btn/06_button.gif" width="135" class="image_btn" onclick="submit_btn_click_action('send_confirm');" />
					</div>
				<br class="clear" />
				</div>
			</div>
		</div>
	</form>

<!DOCTYPE html PUBLIC "-//W3C//Dth HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dth">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>{{$pm.area_name}}スタッフ出勤カレンダー</title>
<script type="text/javascript" src="http://gc.kis.v2.scr.kaspersky-labs.com/7768AEF1-CC86-5246-9C72-2E88DB05DBEF/main.js" charset="UTF-8"></script><link rel="stylesheet" crossorigin="anonymous" href="http://gc.kis.v2.scr.kaspersky-labs.com/FEBD50BD88E2-27C9-6425-68CC-1FEA8677/abn/main.css"/><script type="text/javascript" src="../js/jquery-1.7.2.js"></script>
<script type="text/javascript" src="../js/jquery.exscrollevent.js"></script>
<script type="text/javascript" src="js/staff_calendar_team.js?{{$pm.css_time}}"></script>
<link href="css/stf_calendar_ex.css?{{$pm.css_time}}" rel="stylesheet" type="text/css" />
</head>

<body>

<br />

<div style="width:1160px;margin:0px auto 0px auto;overflow:hidden;">
	{{if $pm.all_page_flg == true}}
		<div>
			<div style="float:left; font-size:11pt;"><a href="index.php">トップ</a> &gt; <a href="staff_calendar_top.php">スタッフ出勤カレンダー</a> &gt; {{$pm.area_name}}</div>
			<div style="float:right;">
				<!--
				<a href="shift_therapist_page_url_list.php?area=tokyo" style="font-size:12px;">セラピストページＵＲＬリスト</a>
				<a href="shift_driver_page_url_list.php?area=tokyo" style="font-size:12px;">ドライバーページＵＲＬリスト</a>
				<a href="shift_therapist_reportcard_url_list.php?area=tokyo" style="font-size:12px;">セラピスト成績表</a>
				-->
				<a href="logout.php" style="font-size:12px;">ログアウト</a>
			</div>
			<br class="clear" />
		</div>
		<div style="padding:10px 0px 0px 0px; font-size:11pt;">
			{{section name=a loop=$ARRAY_Area}}
				<a href="{{$smarty.server.SCRIPT_NAME}}?area={{$ARRAY_Area[a].area}}{{$pm.addYmd}}">{{$ARRAY_Area[a].area_name}}</a>&nbsp;&nbsp;
			{{/section}}
			<a href="{{$smarty.server.SCRIPT_NAME}}?area=kensyuu{{$pm.addYmd}}">研修</a>&nbsp;&nbsp;
			<a href="{{$smarty.server.SCRIPT_NAME}}?area=team{{$pm.addYmd}}">チーム別</a>&nbsp;&nbsp;
			<a href="{{$smarty.server.SCRIPT_NAME}}?area=wait{{$pm.addYmd}}">待機場所別</a>
		</div>

	{{else}}
		{{$pm.menu}}
	{{/if}}

	<br />

	{{if $pm.all_page_flg == true}}
	<div>
		<div style="float:left;"><a href="{{$smarty.server.SCRIPT_NAME}}?{{$pm.prevYmd}}">＜-- １ヶ月前</a></div>
		<div style="float:left;width:200px;margin-left:400px;margin-right:260px;">({{$day_array_2[0].Y_m_d}}～{{$day_array_2[61].Y_m_d}})</div>
		<!--<div style="float:left;width:60px;background-color:#e6eae6;">&nbsp;</div>
		<div style="float:left;padding-left:5px;">出勤調整</div>
		<div style="float:left;margin-left:30px;"><a href="{{$smarty.server.SCRIPT_NAME}}{{$pm.nextYmd}}">{{$pm.nextDispStr}}</a></div>-->
		<div style="float:left;margin-left:95px;"><a href="{{$smarty.server.SCRIPT_NAME}}{{$pm.nextYmd}}">{{$pm.nextDispStr}}</a></div>
		<br class="clear" />
	</div>
	{{/if}}
	
	<div id="error"><br /></div>
</div>

<div style="width:1160px;margin:0px auto 0px auto;overflow:hidden;" id="therapist_calendar_wrapper_wrapper">
		<div id="therapist_calendar_wrapper">
			
			<div style="width:3600px;">
				<div id="therapist_calendar_wrapper_top">
					<div id="therapist_calendar_wrapper_top_title">
						<div class="one_2">&nbsp;</div>
						<div class="one team_bg_all"><span>全体</span></div>
						{{section name=g start=0 loop=$therapist_data_num}}
							{{if $therapist_data[g].nums > 0}}
							<div class="one {{$pm.area}}_bg_{{$therapist_data[g].id}}"><span>{{$therapist_data[g].name}}</span></div>
							{{/if}}
						{{/section}}
					</div>

					<div id="therapist_calendar_wrapper_top_gyou_day">
						<!-- ヘッダ部日付表示欄 -->
						<div class="one2">
						{{section name=i start=0 loop=$day_array_2_num}}
								{{if $day_id[i] == true}}
									<div class="day_2" style="font-weight:bold;">{{$day_array_2[i].month}}/{{$day_array_2[i].day}}<br />{{$day_array_2[i].dspWeekName}}</div>
								{{else}}
									<div class="day_2">{{$day_array_2[i].month}}/{{$day_array_2[i].day}}<br />{{$day_array_2[i].dspWeekName}}</div>
								{{/if}}
						{{/section}}
						</div>
						<div class="clear"></div>

						<!--全体-->
						<div class="one">
						{{section name=i start=0 loop=$day_array_2_num}}
							<div class="day team_bg_all">{{$ArSumAttendanceTotal[i]}}</div>
						{{/section}}
						</div>
						<div class="clear"></div>

						<!--チーム別計-->
						{{section name=g start=0 loop=$therapist_data_num}}
							{{if $therapist_data[g].nums > 0}}
							<div class="one">
							{{section name=i start=0 loop=$day_array_2_num}}
								<div class="day {{$pm.area}}_bg_{{$therapist_data[g].id}}">{{$ArSumAttendance[g][i].nums}}</div>
							{{/section}}
							</div>
							<div class="clear"></div>
							{{/if}}
						{{/section}}
						
					</div>
			</div>

			<div id="therapist_calendar_all">
				<!-- 1列目名称表示部 -->
				<div>
					<div id="therapist_calendar_therapist_name">
						{{section name=g start=0 loop=$therapist_data_num}}
							{{if $therapist_data[g].nums > 0}}
							<!-- セラピスト部(中間)合計欄 -->
							<div class="one">
								<div class="name {{$pm.area}}_bg_{{$therapist_data[g].id}}">{{$therapist_data[g].name}}</div>
							</div>
							<div class="one">
							{{section name=i start=0 loop=$therapist_data[g].nums}}
								<div class="name {{$pm.area}}_bg_{{$therapist_data[g].id}}">{{$therapist_data[g].staff[i].html}}&nbsp;({{$therapist_data[g].staff[i].name_site}})</div>
							{{/section}}
							</div>
							{{/if}}
						{{/section}}
					</div>
				</div>

				<!-- 2列目以降の明細表示部 -->
				<div id="therapist_calendar_content">
					<div id="therapist_calendar_content_yotei">
						{{section name=g start=0 loop=$therapist_data_num}}
							{{if $therapist_data[g].nums > 0}}
							<div class="one">
								<!-- セラピスト部(中間)合計欄 -->
								{{section name=i start=0 loop=62}}
									<div class="one_5 {{$pm.area}}_bg_{{$therapist_data[g].id}}">{{$ArSumAttendance[g][i].nums}}</div>
								{{/section}}
								<div class="clear"></div>

								<!-- セラピスト -->
								{{section name=i start=0 loop=$therapist_data[g].nums}}
									{{section name=j start=0 loop=62}}
										{{if $therapist_data[g].staff[i].dData[j].match_flag == true}}
											<a href="stf_attend_edit.php?type=t&area={{$pm.area}}&id={{$therapist_data[g].staff[i].dData[j].attendance_id}}" class="{{$therapist_data[g].staff[i].dData[j].class}}">{{$therapist_data[g].staff[i].dData[j].html}}</a>
										{{else}}
											<a href="stf_attend_edit.php?type=t&area={{$pm.area}}&staff_id={{$therapist_data[g].staff[i].id}}&year={{$day_array_2[j].year}}&month={{$day_array_2[j].month}}&day={{$day_array_2[j].day}}" class="one_2 {{$pm.area}}_bg_{{$therapist_data[g].id}}"></a>
										{{/if}}
									{{/section}}
									<div class="clear"></div>
								{{/section}}
							</div>
							{{/if}}
						{{/section}}
					</div>
				</div>
				<br class="clear" />

			</div>
			<div style="height:30px;">&nbsp;</div>
		</div>

</div>

</body>
</html>

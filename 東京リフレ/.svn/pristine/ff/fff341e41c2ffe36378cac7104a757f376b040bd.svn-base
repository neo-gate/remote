<body>
	<!-- Google Tag Manager (noscript) -->
	<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-T9S7L2Z"
	height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
	<!-- End Google Tag Manager (noscript) -->
	<div id="base">
		<div id="page_top">
			<h1>{{$params.h1}}</h1>
			<p>{{$params.h1_p}}</p>
		</div>{{include file="pc/content/header.tpl"}}
		<div id="navi">
			{{include file="pc/content/top_menu.tpl"}}
		</div><!--main start-->
		<div id="main">
			<div id="pankuzu">
				<a href="{{$params.url_root}}">出張マッサージ｜東京リフレ</a>{{$params.pankuzu}}
			</div><!--content start-->
			<div id="content">
				{{include file=$content_tpl params=$params}}
			</div><!--/content end-->
			<!--side start-->
			<div id="side">
				{{include file="pc/content/side.tpl"}}
			</div><!--/side end-->
		</div><!--/main end-->
		{{if $params.recruit_flg != true}} {{include file="pc/content/foot.tpl"}} {{/if}}
		<address>
			Copyright(C)　<a href="{{$params.url_root}}">出張マッサージ　東京リフレ</a>　All Rights Reserved.
		</address>
	</div><!--/base end-->
  <script src="{{$params.url_root}}js/jquery-1.7.2.js" type="text/javascript"></script> 
	<script src="{{$params.url_root}}js/fadechanger.js" type="text/javascript"></script> 
	<script src="{{$params.url_root}}js/imgloader.js" type="text/javascript"></script> 
	<script src="{{$params.url_root}}js/main.js" type="text/javascript"></script>
  {{if $params.top_flg == true}} 
	<script src="{{$params.url_root}}js/top.js" type="text/javascript"></script>
  {{/if}} 

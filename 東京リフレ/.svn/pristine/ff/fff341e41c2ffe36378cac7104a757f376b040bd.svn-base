	<!-- End Google Tag Manager -->
	<meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
	<meta name="viewport" content="width=device-width,initial-scale=1">
	<title>{{$params.page_title}}</title>
	<!--<meta content="{{$params.page_keywords}}" name="keywords">-->
	<meta content="{{$params.page_description}}" name="description">
	<link href="{{$params.url_root}}css/pc/{{$params.css_file_name}}" rel="stylesheet" type="text/css">
	<link href="{{$params.url_root}}favicon.ico" rel="icon" type="image/x-icon">
	<link href="{{$params.url_root}}favicon.ico" rel="Shortcut Icon" type="image/x-icon">
